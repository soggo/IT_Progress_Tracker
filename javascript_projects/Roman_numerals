function convertToRoman(num) {
    let table1 = [['M', 1000], ['CM', 900], ['D', 500], ['CD', 400], ['C', 100], ['XC', 90], ['L', 50], ['XL', 40], ['X', 10], ['IX', 9], ['V', 5], ['IV', 4], ['I', 1]];
    let table2 = [];

    for(let i = 0; i<table1.length; i++){
      while( num >= table1[i][1]){
    table2.push(table1[i][0])
     num -= table1[i][1]; 
      } 
    }
      return table2.join('');
  }
  